{
  "name": "ovh-module-emailpro",
  "version": "7.0.3",
  "description": "E-mail Pro module.",
  "author": "OVH SAS",
  "license": "BSD-3-Clause",
  "repository": "ovh-ux/ovh-module-emailpro",
  "keywords": [
    "angular",
    "e-mail pro"
  ],
  "engines": {
    "node": ">=8",
    "yarn": ">= 1.0.0"
  },
  "scripts": {
    "test": "yarn run lint",
    "lint": "run-p lint:css lint:js",
    "lint:css": "stylelint 'src/**/*.less' --fix",
    "lint:js": "eslint --quiet --fix ./src",
    "version": "npx conventional-changelog-cli -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "git push && git push --tags"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.less": [
      "stylelint 'src/**/*.less' --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "angular": "~1.6.10",
    "lodash": "~3.9.3",
    "moment": "^2.16.0",
    "punycode": "^1.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-angular": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-angularjs-annotate": "^0.9.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-angular": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.1.0",
    "lint-staged": "^7.3.0",
    "npm-run-all": "^4.1.3",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0"
  }
}
