{
  "root": true,
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "plugins": ["angular"],
  "env": {
    "browser": true,
    "jasmine": true,
    "mocha": true,
    "jquery": true,
    "angular/mocks": true
  },
  "globals": {
    "_": true,
    "CKEDITOR": true,
    "moment": true,
    "punycode": true,
    "validator": true
  },
  "rules": {
    "import/no-unresolved": 0
  }
}
